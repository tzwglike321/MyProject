	JButton b2=new JButton("开始运行");
	p2.setLayout(new FlowLayout());
	p2.add(b1);
	p2.add(b2);
	j.setLayout(new BorderLayout());
	j.add(p1,BorderLayout.NORTH);
	j.add(p2,BorderLayout.CENTER);
	j.setLocationRelativeTo(null);
